FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 600;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value		uniform 600;
    }

    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }

    defaultFaces
    {
        type            zeroGradient;
    }

    fsi_surface
    {
        type            zeroGradient;
    }
}

